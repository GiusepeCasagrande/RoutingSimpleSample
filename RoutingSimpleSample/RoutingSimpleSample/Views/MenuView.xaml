<?xml version="1.0" encoding="UTF-8"?>
<ui:ContentPageBase xmlns="http://xamarin.com/schemas/2014/forms" 
		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
		x:TypeArguments="vms:MenuViewModel" 
		xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms" 
		xmlns:ui="clr-namespace:RoutingSimpleSample.Views;assembly=RoutingSimpleSample" 
		xmlns:vms="clr-namespace:RoutingSimpleSample.ViewModels;assembly=RoutingSimpleSample" 
		x:Class="RoutingSimpleSample.Views.MenuView">
		<ListView SeparatorVisibility="None" x:Name="MenuList">
			<ListView.ItemTemplate>
				<DataTemplate>
						<ui:MenuCellView ViewModel="{Binding .}"/>
					</DataTemplate>
			</ListView.ItemTemplate>
		</ListView>
</ui:ContentPageBase>
