<?xml version="1.0" encoding="UTF-8"?>
<ui:ContentPageBase xmlns="http://xamarin.com/schemas/2014/forms" 
		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
		x:TypeArguments="vms:SamplePageViewModel" 
		xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms" 
		xmlns:ui="clr-namespace:RoutingSimpleSample.Views;assembly=RoutingSimpleSample" 
		xmlns:vms="clr-namespace:RoutingSimpleSample.ViewModels;assembly=RoutingSimpleSample" 
		x:Class="RoutingSimpleSample.Views.SamplePageView">
		<StackLayout Orientation="Horizontal" Padding="20,0,0,0">
			<Label x:Name="PageIcon" HorizontalOptions="Start" VerticalOptions="CenterAndExpand" FontFamily="FontAwesome" FontSize="32" />
			<Label x:Name="PageName" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
		</StackLayout>
</ui:ContentPageBase>
	